    "    answer = ''\n",
    "    completion.sort()\n",
    "    completion.append('')\n",
    "    for a,i in enumerate(sorted(participant)):\n",
    "        if i != completion[a]:\n",
    "            answer=i\n",
    "            break\n",
    "    return answer"
   ]
