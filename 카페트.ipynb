    "def solution(brown, red):\n",
    "    for i in range(1,red//2+2) :\n",
    "        for j in reversed(range(red//i+1)) :\n",
    "            if i*j == red :\n",
    "                if 2*(i+j)+4 == brown :\n",
    "                    return [max(i,j)+2, min(i,j)+2]\n",
    "                break\n",
    "    return"
